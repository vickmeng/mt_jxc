{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "member-access": false,
    "interface-name": false,
    "no-empty-interface": false,
    "max-classes-per-file": false,
    "jsx-boolean-value": false,
    "jsx-no-lambda":false,
    "no-console":false
  }
}
